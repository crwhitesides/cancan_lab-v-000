<h1>Welcome, <%= current_user.name %></h1>
<%= button_to "Sign Out", logout_path %><br>

<%= link_to "Send New Note", new_note_path %>

<p>Notes received:</p>
<% @notes_received.each do |note| %>
  <ul>
    <li><%= note.user.name %> said: <%= note.content %></li>
  </ul>
<% end %>

<p>Notes sent:</p>
<% @notes_sent.each do |note| %>
  <ul>
    <li>To <%= note.visible_to %>: <%= note.content %></li>
  </ul>
<% end %>
